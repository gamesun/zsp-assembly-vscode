{
    "name": "zsp-assembly",
    "displayName": "ZSP Assembly",
    "description": "ZSP assembly language support for Visual Studio Code",
    "version": "1.0.1",
    "homepage": "https://github.com/gamesun/zsp-assembly-vscode",
    "icon": "icon.png",
    "publisher": "gamesun",
    "license": "MIT",
    "author": {
		"name": "gamesun"
	},
    "repository": {
        "url": "https://github.com/gamesun/zsp-assembly-vscode",
        "type": "git"
    },
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zsp-assembly",
            "aliases": ["ZSP Assembly", "zsp-assembly"],
            "extensions": [".s",".S"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zsp-assembly",
            "scopeName": "source.zsp-assembly",
            "path": "./syntaxes/zsp-assembly.tmLanguage.json"
        }]
    }
}
